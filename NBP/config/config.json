{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "currency-rate-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/currencyRate/CurrencyRateWebPart.js",
          "manifest": "./src/webparts/currencyRate/CurrencyRateWebPart.manifest.json"
        }
      ]
    },
    "nbp-extension-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/nbpExtension/NbpExtensionApplicationCustomizer.js",
          "manifest": "./src/extensions/nbpExtension/NbpExtensionApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "CurrencyRateWebPartStrings": "lib/webparts/currencyRate/loc/{locale}.js",
    "NbpExtensionApplicationCustomizerStrings": "lib/extensions/nbpExtension/loc/{locale}.js"
  }
}
